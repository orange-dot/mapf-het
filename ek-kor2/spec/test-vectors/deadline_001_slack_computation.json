{
  "id": "deadline_001",
  "name": "deadline_slack_computation",
  "module": "deadline",
  "function": "compute_slack",
  "description": "Compute slack for a task with deadline in the future",

  "input": {
    "deadline": 110000000,
    "duration_est": 5000000,
    "now": 0
  },

  "expected": {
    "slack_us": 105000000,
    "slack_normalized": 1.0,
    "critical": false
  },

  "notes": [
    "slack_us = deadline - (now + duration_est) = 110000000 - (0 + 5000000) = 105000000",
    "slack_normalized = min(slack_us / 100000000, 1.0) = 1.0 (capped at max)",
    "critical = slack_us < 10000000 = false (105s > 10s threshold)"
  ]
}
